{
  "name": "latest-joi-to-swagger",
  "version": "1.0.1",
  "description": "[Updated] A lightweight conversion library for transforming Joi schema objects into Swagger/OpenAPI OAS 3.0 schema definitions. Designed for developers looking to automate API documentation generation while maintaining flexibility and accuracy in schema validation.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "tap tests.js",
    "test:coverage:html": "tap --coverage-report=html tests.js",
    "test:debug": "tap --node-arg=--inspect --no-timeout tests.js",
    "test:ci": "tap --reporter=tap tests.js",
    "lint": "eslint ./"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "joi",
    "swagger",
    "openapi",
    "oas",
    "schema",
    "conversion",
    "validation",
    "api",
    "documentation"
  ],
  "bugs": {
    "url": "https://github.com/cyrus-reap/latest-joi-to-swagger/issues"
  },
  "homepage": "https://github.com/cyrus-reap/latest-joi-to-swagger#readme",
  "author": "Cyrus Pastelero <cyrus@reap.hk>",
  "license": "MIT",
  "devDependencies": {
    "@joi/date": "^2.1.0",
    "eslint": "^7.32.0",
    "eslint-config-twipped": "~3.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "joi": "^17.13.3",
    "tap": "^16.3.2",
    "tapsuite": "^2.0.1"
  },
  "peerDependencies": {
    "joi": ">=17.1.1"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
